{
    "name": "solcli",
    "version": "0.0.1",
    "description": "Ultimate cli for Solidity projects",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": "https://github.com/jeroenouw/solcli.git",
    "author": "Jeroen Ouwehand",
    "license": "MIT",
    "scripts": {
        "start": "npm run build && npm run global",
        "build": " rm -rf ./lib && tsc -p .",
        "global": "npm i -g && solcli",
        "reset": "rm -rf ./node_modules ./package-lock.json ./lib && npm install",
        "tscov": "tscov -m 90",
        "tscov:d": "tscov -d"
    },
    "bin": {
        "solcli": "bin/solcli"
    },
    "keywords": [
        "cli",
        "solidity",
        "solcli",
        "typescript",
        "generator",
        "nodejs"
    ],
    "dependencies": {
        "commander": "^2.19.0",
        "figlet": "^1.2.1",
        "fs-extra": "^7.0.1",
        "glob": "7.1.3",
        "inversify": "^5.0.1",
        "minimist": "1.2.0",
        "prompts": "^2.0.3",
        "reflect-metadata": "^0.1.13",
        "tslib": "1.9.3"
    },
    "devDependencies": {
        "@types/fs-extra": "^5.0.5",
        "@types/glob": "7.1.1",
        "@types/jasmine": "3.3.9",
        "@types/minimist": "1.2.0",
        "@types/node": "11.9.4",
        "jasmine": "3.3.1",
        "no-unused-export": "1.7.0",
        "standard": "12.0.1",
        "ts-node": "^8.0.2",
        "tscov": "^1.3.3",
        "typescript": "^3.3.3"
    }
}
